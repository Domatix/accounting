<?xml version="1.0" encoding="utf-8"?>
<openerp>
     <data>

        <record id="account_move_line_customize" model="ir.ui.view">
            <field name="name">account.move.line.customize</field>
            <field name="model">account.move.line</field>
            <field name="inherit_id" ref="account.view_move_line_tree" />
            <field name="arch" type="xml">

              <field name="reconcile_ref" position="before">
                <field name="date"/>
                <field name="period_id" options="{&quot;no_open&quot;:True}"/>
                <field name="move_id" required="0"/>
                <field name="name"/>
                <field name="debit" sum="Total Debit"/>
                <field name="credit" sum="Total Credit"/>
                <field name="date_maturity"/>
                <field name="journal_id" options="{&quot;no_open&quot;:True}"/>
              </field>
                <field name="journal_id" position="replace"/>
                <field name="period_id" position="replace"/>
                <field name="date" position="replace"/>
                <field name="name" position="replace"/>
                <field name="ref" position="replace"/>
                <field name="partner_id" position="replace"/>
                <field name="account_id" position="replace"/>
                <field name="account_tax_id" position="replace"/>
                <field name="move_id" position="replace"/>
                <field name="debit" position="replace"/>
                <field name="credit" position="replace"/>
                <field name="date_maturity" position="replace"/>
                <field name="reconcile_ref" position="replace"/>
                <field name="amount_currency" position="replace"/>
                <field name="currency_id" position="replace"/>
            </field>
        </record>

        <record id="account_move_line_customizee" model="ir.ui.view">
            <field name="name">account.move.line.customizee</field>
            <field name="model">account.move.line</field>
            <field name="inherit_id" ref="account_analytic_plans.view_move_line_tree_inherit" />
            <field name="arch" type="xml">
              <field name="analytics_id" position="replace">
              </field>

            </field>
        </record>

    </data>
</openerp>
